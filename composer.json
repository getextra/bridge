{
	"name": "wpextra/bridge",
	"type": "library",
	"description": "Plugin to write WordPress themes w Object-Oriented Code and the Twig Template Engine",
	"keywords": [
		"timber",
		"twig",
		"themes",
		"templating"
	],
	"require": {
		"php": ">=5.3.0|7.*",
		"twig/twig": "1.34.*",
		"asm89/twig-cache-extension": "~1.0",
		"symfony/routing": "^4.2@dev",
		"symfony/http-foundation": "^4.2@dev",
		"doctrine/annotations": "1.7.x-dev",
		"symfony/property-info": "^4.2@dev"
	},
	"require-dev": {
		"phpunit/phpunit": "5.7.16"
	},
	"support": {
		"issues": "https://github.com/wpextra/bridge/issues",
		"source": "https://github.com/wpextra/bridge",
		"docs": "https://bridge.github.io/docs/"
	},
	"homepage": "http://wpextra.co",
	"authors": [
		{
			"name": "WP EXTRA",
			"email": "developer@wpextra.co"
		}
	],
	"autoload": {
		"files": [
			"src/Kernel.php"
		]
	},
	"minimum-stability": "dev"
}
